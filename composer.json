{
    "name": "logipro/emailsender-client",
    "type": "library",
    "description": "A PHP component to use Sms sender API within your PHP project.",
    "keywords": ["EmailSender"],
    "homepage": "https://github.com/logipro-fr/emailsender-client",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Besse Pedro",
            "email": "pedro.besse@logipro.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/http-client": "^6.4"
    },
    "require-dev": {
        "infection/extension-installer": "0.1.2",
        "infection/infection": "^0.27",
        "phpstan/phpdoc-parser": "^1.20",
        "phpstan/phpstan": "1.10.15",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "3.*"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "EmailSenderClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmailSenderClient\\Tests\\": "tests/unit"
        }
    }
}
